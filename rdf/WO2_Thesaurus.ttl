<https://data.niod.nl/WO2_Thesaurus/corporaties/4849> dcterms:contributor <https://cultureelerfgoed.poolparty.biz/user/jongmal> ;
	dcterms:created "2016-10-04T13:07:44Z"^^xsd:dateTime ;
