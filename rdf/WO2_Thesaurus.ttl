<https://data.niod.nl/WO2_Thesaurus/1740> dcterms:contributor <https://cultureelerfgoed.poolparty.biz/user/jongmal> ;
	dcterms:created "2016-08-01T19:19:14Z"^^xsd:dateTime ;
