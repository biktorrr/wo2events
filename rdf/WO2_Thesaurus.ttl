<https://data.niod.nl/WO2_Thesaurus/1951> dcterms:contributor <https://cultureelerfgoed.poolparty.biz/user/jongmal> ;
	dcterms:created "2016-08-01T19:19:32Z"^^xsd:dateTime ;
